{
    "notifications": {
        "timeout_low": 4000,
        "timeout_normal": 6000,
        "timeout_critical": 0,

        "position_h": "right",
        "position_v": "top",
        "dismiss_on_unhover": true
    },
    "night_light": {
        "save_on_shutdown": true
    },
    "workspaces": {
        "always_show_id": false,
        "enable_helper": true,
        "show_id_on_hover": false,
        "hide_if_single": true
    },
    "wallpaper": {
        "color_mode": "darken",
        "positioning": "contain"
    },
    "apps": {
        "term": "kitty",
        "media": "spotify-launcher"
    },
    "clock": {
        "date_format": "%A %d, %H:%M"
    },
    "misc": {
        "play_bell_on_volume_change": true
    }
}
